{
  "java.configuration.updateBuildConfiguration": "automatic",
  "maven.executable.path": "${env.M2_HOME}/bin/mvn",
  "maven.terminal.customEnv": [
    {
      "environmentVariable": "JAVA_HOME",
      "value": "${env.JAVA_HOME}"
    }
  ],
  "java.compile.nullAnalysis.mode": "automatic",
  "java.format.settings.url": ".vscode/eclipse-java-google-style.xml",
  "java.format.settings.profile": "GoogleStyle",
  "java.completion.importOrder": [
    "java",
    "javax",
    "org",
    "com"
  ],
  "java.debug.settings.onBuildFailureProceed": true,
  "java.project.referencedLibraries": [
    "lib/**/*.jar"
  ],
  "java.configuration.maven.userSettings": "${env.M2_HOME}/conf/settings.xml",
  "java.home": "${env.JAVA_HOME}",
  "maven.settingsFile": "${env.M2_HOME}/conf/settings.xml"
}
